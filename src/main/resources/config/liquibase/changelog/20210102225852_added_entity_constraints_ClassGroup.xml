<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity ClassGroup.
    -->
    <changeSet id="20210102225852-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="course_id"
                                 baseTableName="class_group"
                                 constraintName="fk_class_group_course_id"
                                 referencedColumnNames="id"
                                 referencedTableName="course"/>

        <addForeignKeyConstraint baseColumnNames="lecturer_id"
                                 baseTableName="class_group"
                                 constraintName="fk_class_group_lecturer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lecturer"/>

    </changeSet>
</databaseChangeLog>
